{
  "name": "ai-interview-assistant",
  "version": "1.0.0",
  "description": "A professional AI-powered interview application featuring real-time 3D avatar interactions, speech-to-text transcription, and intelligent question management.",
  "keywords": [
    "ai-interview",
    "3d-avatar",
    "speech-recognition",
    "nextjs",
    "react-three-fiber",
    "google-ai",
    "interview-assistant"
  ],
  "author": "AI Interview Team",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "docker:build": "docker build -t ai-interview .",
    "docker:run": "docker run -p 3000:3000 ai-interview",
    "vercel:deploy": "vercel --prod",
    "deploy:docker": "bash scripts/deploy.sh docker",
    "deploy:compose": "bash scripts/deploy.sh compose",
    "deploy:vercel": "bash scripts/deploy.sh vercel",
    "deploy:traditional": "bash scripts/deploy.sh traditional"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@react-three/drei": "^10.7.4",
    "@react-three/fiber": "^9.3.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "three": "^0.179.1",
    "zustand": "^5.0.8",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "rate-limiter-flexible": "^4.0.1",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "typescript": "^5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "playwright": "^1.40.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "@next/bundle-analyzer": "^15.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-interview.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-interview/issues"
  },
  "homepage": "https://github.com/yourusername/ai-interview#readme"
}
